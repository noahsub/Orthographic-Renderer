<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="200"
        x:Class="Orthographic.Renderer.Windows.RenderComplete"
        Title="RenderComplete"
        Width="300"
        Height="100">
    <Grid Background="#202124">
        <StackPanel HorizontalAlignment="Stretch">
            <Label Content="Render Complete" Margin="20 0 0 0"/>
            <Label x:Name="TimeLabel" Content="0:00:00" Margin="20 0 0 0"/>
            <Button x:Name="CloseButton" Content="Close" Click="CloseButton_OnClick" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
